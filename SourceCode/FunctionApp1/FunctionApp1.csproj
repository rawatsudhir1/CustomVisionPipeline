    <UserSecretsId>6196ffc3-9f1f-45de-b27d-eec042ae7d0c</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Azure.Storage.Blobs" Version="12.8.0" />
